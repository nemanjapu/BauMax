<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title - BauMax Instalacija sistema za klimatizaciju, vodovod i kanalizaciju</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1.0, shrink-to-fit=no">
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <link rel="manifest" href="/site.webmanifest">
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="theme-color" content="#ffffff">
    <meta property="og:image" content="http://www.baumax.ba/Content/images/construction/logo.png">
    <meta property="og:image:type" content="image/png">
    @RenderSection("meta", required: false)
    <meta name="author" content="struix.co">
    @Html.Partial("_PreloadResouresPartial")

    <link href="~/Content/bundle.min.css" rel="stylesheet" />
    <script src="~/Scripts/modernizr-2.8.3.min.js"></script>
    @RenderSection("styles", required: false)
</head>
<body data-spy="scroll" data-target="#sidebar" data-offset="120">
    <div class="body">
        @Html.Partial("_MenuPartial")
        @RenderBody()
        @Html.Partial("_FooterPartial")
    </div>

    <script src="~/Scripts/bundle.min.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
